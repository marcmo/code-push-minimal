{
  "name": "Minimal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "run:ios": "react-native run-ios",
    "run:android": "react-native run-android",
    "git-hook": "npm run lint -s && npm run test -s"
  },
  "dependencies": {
    "apisauce": "^0.14.0",
    "base-64": "^0.1.0",
    "js-md5": "^0.7.2",
    "lottie-react-native": "2.2.7",
    "prop-types": "^15.5.10",
    "protobufjs": "^6.8.0",
    "ramda": "^0.24.1",
    "react": "16.0.0",
    "react-native": "0.47.2",
    "react-native-action-button": "^2.8.1",
    "react-native-code-push": "^5.2.0-beta",
    "react-native-config": "^0.6.0",
    "react-native-device-info": "0.11.0",
    "react-native-fabric": "^0.5.1",
    "react-native-interactable": "^0.1.7",
    "react-native-linear-gradient": "^2.3.0",
    "react-native-maps": "^0.19.0",
    "react-native-navigation": "^1.1.237",
    "react-native-vector-icons": "^4.4.2",
    "react-redux": "^5.0.6",
    "recyclerlistview": "^1.2.2",
    "redux": "v4.0.0-beta.1",
    "redux-form": "^7.1.2",
    "redux-persist": "^5.4.0",
    "redux-saga": "^0.16.0",
    "xstate": "^1.2.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.0",
    "@types/jest": "^21.1.8",
    "@types/ramda": "^0.25.7",
    "@types/react": "^16.0.34",
    "@types/react-native": "^0.48.7",
    "@types/react-redux": "^5.0.14",
    "@types/redux-form": "^7.0.8",
    "babel-eslint": "7.1.1",
    "babel-jest": "21.0.2",
    "babel-plugin-ignite-ignore-reactotron": "^0.3.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react-native": "3.0.2",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "husky": "^0.13.1",
    "jest": "21.1.0",
    "mockery": "^2.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "16.0.0-alpha.12",
    "react-native-typescript-transformer": "^1.1.4",
    "react-test-renderer": "16.0.0-alpha.12",
    "reactotron-react-native": "^1.12.3",
    "reactotron-redux": "^1.11.1",
    "reactotron-redux-saga": "^1.11.1",
    "snazzy": "^7.0.0",
    "ts-jest": "^21.2.4",
    "ts-node": "^3.3.0",
    "tslib": "^1.7.1",
    "tslint": "^5.7.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.6.2",
    "typescript-formatter": "^6.0.0"
  },
  "jest": {
    "preset": "react-native",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "base-64": "<rootDir>/node_modules/base-64/base64.js"
    },
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "setupFiles": [
      "./setupTests.js"
    ]
  },
  "config": {
  }
}